{
	"npmScope": "aegis-auth",
	"affected": {
		"defaultBase": "master"
	},
	"implicitDependencies": {
		"angular.json": "*",
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		},
		"tsconfig.base.json": "*",
		"tsconfig.json": "*",
		"eslint.json": "*",
		".eslintrc.json": "*",
		"nx.json": "*"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "nx/tasks-runners/default",
			"options": {
				"cacheableOperations": ["build", "lint", "test", "e2e"],
				"runtimeCacheInputs": ["node -v"],
				"cacheDirectory": ".cache/nx",
				"parallel": 1
			}
		}
	},
	"targetDependencies": {
		"build": [
			{
				"target": "build",
				"projects": "dependencies"
			}
		]
	},
	"cli": {
		"defaultCollection": "@nrwl/angular",
		"analytics": false
	},
	"defaultProject": "core",
	"generators": {
		"@schematics/angular": {
			"component": {
				"changeDetection": "OnPush",
				"style": "scss",
				"flat": true
			}
		},
		"@nrwl/angular": {
			"application": {
				"linter": "eslint"
			},
			"library": {
				"linter": "eslint"
			},
			"storybook-configuration": {
				"linter": "eslint"
			}
		},
		"@nrwl/angular:application": {
			"unitTestRunner": "jest",
			"e2eTestRunner": "cypress"
		},
		"@nrwl/angular:library": {
			"unitTestRunner": "jest"
		}
	}
}
