{
	"configurations": [
		{
			"type": "chrome",
			"request": "launch",
			"name": "Launch Core Demo",
			"url": "http://localhost:4801",
			"webRoot": "${workspaceFolder}",
			"cwd": "${workspaceFolder}",
			"preLaunchTask": "start:core"
		},
		{
			"type": "chrome",
			"request": "launch",
			"name": "Launch Jwt Demo",
			"url": "http://localhost:4802",
			"webRoot": "${workspaceFolder}",
			"cwd": "${workspaceFolder}",
			"preLaunchTask": "start:jwt"
		},
		{
			"type": "chrome",
			"request": "launch",
			"name": "Launch Ngrx Demo",
			"url": "http://localhost:4802",
			"webRoot": "${workspaceFolder}",
			"cwd": "${workspaceFolder}",
			"preLaunchTask": "start:ngrx"
		},
		{
			"name": "Current TS File",
			"type": "pwa-node",
			"request": "launch",
			"sourceMaps": true,
			"smartStep": true,
			"args": ["${relativeFile}"],
			"runtimeArgs": ["-r", "ts-node/register", "-r", "tsconfig-paths/register"],
			"cwd": "${workspaceRoot}",
			"internalConsoleOptions": "openOnSessionStart"
		}
	]
}
