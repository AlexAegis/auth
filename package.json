{
	"name": "aegis-auth",
	"description": "Angular authentication libraries",
	"version": "11.0.0",
	"license": "MIT",
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/auth#readme",
	"repository": "github:AlexAegis/auth",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/auth/issues"
	},
	"keywords": [
		"ng",
		"angular",
		"nx",
		"jwt",
		"token",
		"auth",
		"authentication",
		"rxjs",
		"reactive",
		"interceptor",
		"ts",
		"typescript",
		"js",
		"javascript"
	],
	"sideEffects": [
		"./apps/*/src/main.ts"
	],
	"private": true,
	"config": {
		"engine-strict": true
	},
	"engines": {
		"node": ">=10.13.0",
		"yarn": ">=1.0.0"
	},
	"workspaces": {
		"packages": [
			"apps/*",
			"libs/*"
		]
	},
	"scripts": {
		"ng": "ng",
		"nx": "nx",
		"start": "nx serve jwt-demo",
		"start:core": "nx serve core-demo",
		"start:jwt": "nx serve jwt-demo",
		"start:ngrx": "nx serve ngrx-demo",
		"build": "nx affected:build --all --prod",
		"build:core": "nx build core-demo --prod --stats-json",
		"build:jwt": "nx build jwt-demo --prod --stats-json",
		"build:ngrx": "nx build ngrx-demo --prod --stats-json",
		"hoist:core": "cpy ./dist/apps/core-demo/* ./dist",
		"clean": "rimraf ./dist ./coverage ./.codacy-coverage ./cache",
		"self-destruct": "rimraf ./dist ./coverage ./.codacy-coverage ./cache ./node-modules ./package-lock.json ./yarn.lock ./pnpm-lock.yaml",
		"test": "nx run-many --all --target test",
		"test:skip-cache": "nx run-many --all --target test --skip-nx-cache",
		"lint": "nx workspace-lint && ng lint",
		"lint:es": "eslint --max-warnings=0 .",
		"lint:css": "stylelint --allow-empty-input --syntax=css ./**/*.css",
		"lint:scss": "stylelint --allow-empty-input --syntax=scss ./**/*.scss",
		"lint:md": "markdownlint --ignore 'CHANGELOG.md' --ignore-path '.gitignore' .",
		"lint:rmd": "remark .",
		"lint:format": "prettier --check .",
		"e2e": "nx e2e",
		"analyze:jwt": "webpack-bundle-analyzer dist/apps/jwt-demo/stats.json",
		"affected:apps": "nx affected:apps",
		"affected:libs": "nx affected:libs",
		"affected:build": "nx affected:build --prod",
		"affected:e2e": "nx affected:e2e",
		"affected:test": "nx affected:test",
		"affected:lint": "nx affected:lint",
		"affected:dep-graph": "nx affected:dep-graph",
		"affected": "nx affected",
		"format": "nx format:write",
		"format:write": "nx format:write",
		"format:check": "nx format:check",
		"ncu": "ncu -u",
		"typedoc": "typedoc libs --tsconfig ./tsconfig.json --out ./dist/typedoc",
		"update": "ng update @nrwl/workspace",
		"update:all": "nx update --all --force",
		"workspace-schematic": "nx workspace-schematic",
		"dep-graph": "nx dep-graph",
		"help": "nx help",
		"postinstall": "husky install && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points --tsconfig tsconfig.json",
		"release": "standard-version",
		"release:minor": "standard-version --release-as minor",
		"release:major": "standard-version --release-as major",
		"prepublishOnly": "yarn build",
		"prepublish": "pinst --disable",
		"postpublish": "pinst --enable"
	},
	"dependencies": {
		"@angular/animations": "^11.0.2",
		"@angular/cdk": "^11.0.0",
		"@angular/common": "~11.0.2",
		"@angular/compiler": "~11.0.2",
		"@angular/core": "~11.0.2",
		"@angular/forms": "~11.0.2",
		"@angular/material": "^11.0.0",
		"@angular/platform-browser": "~11.0.2",
		"@angular/platform-browser-dynamic": "~11.0.2",
		"@angular/router": "~11.0.2",
		"@fortawesome/angular-fontawesome": "^0.8.0",
		"@fortawesome/fontawesome-svg-core": "^1.2.32",
		"@fortawesome/free-brands-svg-icons": "^5.15.1",
		"@fortawesome/free-solid-svg-icons": "^5.15.1",
		"@nrwl/angular": "10.4.4",
		"core-js": "~3.7.0",
		"rxjs": "~6.6.3",
		"tslib": "^2.0.3",
		"zone.js": "~0.11.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.1100.2",
		"@angular-eslint/builder": "^0.8.0-beta.2",
		"@angular-eslint/eslint-plugin": "^0.8.0-beta.1",
		"@angular-eslint/eslint-plugin-template": "^0.8.0-beta.7",
		"@angular-eslint/schematics": "^0.8.0-beta.2",
		"@angular-eslint/template-parser": "^0.8.0-beta.2",
		"@angular/cli": "~11.0.2",
		"@angular/compiler-cli": "~11.0.2",
		"@angular/language-service": "~11.0.2",
		"@commitlint/cli": "~11.0.0",
		"@commitlint/config-conventional": "~11.0.0",
		"@nrwl/cypress": "~10.4.4",
		"@nrwl/eslint-plugin-nx": "~10.4.4",
		"@nrwl/jest": "~10.4.4",
		"@nrwl/linter": "~10.4.4",
		"@nrwl/node": "~10.4.4",
		"@nrwl/web": "~10.4.4",
		"@nrwl/workspace": "~10.4.4",
		"@types/jest": "~26.0.15",
		"@types/js-base64": "~3.0.0",
		"@types/node": "~14.14.9",
		"@typescript-eslint/eslint-plugin": "~4.8.2",
		"@typescript-eslint/parser": "~4.8.2",
		"codelyzer": "^6.0.1",
		"cpy-cli": "^3.1.1",
		"cypress": "~5.6.0",
		"dotenv": "~8.2.0",
		"eslint": "~7.14.0",
		"eslint-config-prettier": "~6.15.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsdoc": "^30.7.8",
		"eslint-plugin-prefer-arrow": "^1.2.2",
		"eslint-plugin-prettier": "~3.1.4",
		"eslint-plugin-react": "latest",
		"husky": "^5.0.4",
		"jest": "~26.6.3",
		"jest-preset-angular": "8.3.2",
		"lint-staged": "~10.5.2",
		"markdownlint": "~0.21.1",
		"markdownlint-cli": "~0.25.0",
		"ng-packagr": "^11.0.3",
		"npm-check-updates": "~10.2.2",
		"pinst": "^2.1.1",
		"prettier": "~2.2.0",
		"remark": "~13.0.0",
		"remark-cli": "~9.0.0",
		"remark-lint": "~8.0.0",
		"remark-lint-maximum-line-length": "~2.0.3",
		"remark-preset-lint-consistent": "~4.0.0",
		"remark-preset-lint-recommended": "~5.0.0",
		"rimraf": "~3.0.2",
		"rollup": "~2.33.3",
		"rollup-plugin-gzip": "~2.5.0",
		"rollup-plugin-terser": "~7.0.2",
		"standard-version": "~9.0.0",
		"stylelint": "~13.8.0",
		"stylelint-config-prettier": "~8.0.2",
		"stylelint-prettier": "~1.1.2",
		"ts-jest": "~26.4.4",
		"ts-node": "~9.0.0",
		"typedoc": "~0.19.2",
		"typedoc-plugin-external-module-name": "~4.0.3",
		"typescript": "~4.0.3",
		"webpack-bundle-analyzer": "~4.1.0",
		"yaml-lint": "~1.2.4"
	}
}
