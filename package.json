{
	"name": "aegis-auth",
	"description": "Angular authentication libraries",
	"version": "14.0.0",
	"license": "MIT",
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/auth#readme",
	"repository": "github:AlexAegis/auth",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/auth/issues"
	},
	"keywords": [
		"ng",
		"angular",
		"nx",
		"jwt",
		"token",
		"auth",
		"authentication",
		"rxjs",
		"reactive",
		"interceptor",
		"ts",
		"typescript",
		"js",
		"javascript"
	],
	"sideEffects": [
		"./apps/*/src/main.ts"
	],
	"private": true,
	"config": {
		"engine-strict": true
	},
	"engines": {
		"node": ">=14.0.0",
		"npm": ">=7.0.0"
	},
	"workspaces": [
		"apps/*",
		"libs/*"
	],
	"scripts": {
		"ng": "ng",
		"nx": "nx",
		"start": "nx serve jwt-demo",
		"start:core": "nx serve core-demo",
		"start:jwt": "nx serve jwt-demo",
		"start:ngrx": "nx serve ngrx-demo",
		"build": "nx affected:build --all --configuration production",
		"build:core": "nx build core-demo --configuration production --stats-json",
		"build:jwt": "nx build jwt-demo --configuration production --stats-json",
		"build:ngrx": "nx build ngrx-demo --configuration production --stats-json",
		"hoist:core": "cpy 'dist/apps/core-demo/*' 'dist'",
		"clean": "rimraf ./dist ./coverage ./.codacy-coverage ./cache",
		"nuke": "rimraf ./dist ./coverage ./.codacy-coverage ./cache ./node_modules ./package-lock.json ./yarn.lock ./pnpm-lock.yaml",
		"test": "nx run-many --all --target test",
		"test:skip-cache": "nx run-many --all --target test --skip-nx-cache",
		"lint": "nx run-many --all --target=lint",
		"lint:css": "stylelint --allow-empty-input {apps,libs}/**/*.css",
		"lint:scss": "stylelint --allow-empty-input --customSyntax=postcss-scss {apps,libs}/**/*.scss",
		"lint:md": "markdownlint --ignore 'CHANGELOG.md' --ignore-path '.gitignore' .",
		"lint:rmd": "remark .",
		"lint:format": "prettier --check .",
		"e2e": "nx e2e",
		"analyze:jwt": "webpack-bundle-analyzer dist/apps/jwt-demo/stats.json",
		"affected:apps": "nx affected:apps",
		"affected:libs": "nx affected:libs",
		"affected:build": "nx affected:build --configuration production",
		"affected:e2e": "nx affected:e2e",
		"affected:test": "nx affected:test",
		"affected:lint": "nx affected:lint",
		"affected:dep-graph": "nx affected:dep-graph",
		"affected": "nx affected",
		"format": "nx format:write",
		"format:write": "nx format:write",
		"format:check": "nx format:check",
		"ncu": "ncu -u",
		"typedoc": "typedoc --tsconfig ./tsconfig.base.json --out ./dist/typedoc",
		"update": "nx migrate latest",
		"dep-graph": "nx dep-graph",
		"help": "nx help",
		"postinstall": "husky install && node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main --first-only --create-ivy-entry-points",
		"release": "standard-version",
		"release:minor": "standard-version --release-as minor",
		"release:major": "standard-version --release-as major",
		"prepublishOnly": "pinst --disable && npm run build",
		"postpublish": "pinst --enable",
		"workspace-generator": "nx workspace-generator"
	},
	"dependencies": {
		"@angular/animations": "14.0.4",
		"@angular/cdk": "14.0.4",
		"@angular/common": "14.0.4",
		"@angular/compiler": "14.0.4",
		"@angular/core": "14.0.4",
		"@angular/forms": "14.0.4",
		"@angular/material": "14.0.4",
		"@angular/platform-browser": "14.0.4",
		"@angular/platform-browser-dynamic": "14.0.4",
		"@angular/router": "14.0.4",
		"@fortawesome/angular-fontawesome": "^0.11.1",
		"@fortawesome/fontawesome-svg-core": "^6.1.1",
		"@fortawesome/free-brands-svg-icons": "^6.1.1",
		"@fortawesome/free-solid-svg-icons": "^6.1.2",
		"@nrwl/angular": "14.3.6",
		"core-js": "~3.23.3",
		"js-base64": "^3.7.2",
		"rxjs": "~7.5.5",
		"tslib": "^2.4.0",
		"zone.js": "0.11.7"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "14.0.4",
		"@angular-eslint/builder": "14.0.0",
		"@angular-eslint/eslint-plugin": "14.0.0",
		"@angular-eslint/eslint-plugin-template": "14.0.2",
		"@angular-eslint/schematics": "14.0.0",
		"@angular-eslint/template-parser": "14.0.0",
		"@angular/cli": "~14.0.4",
		"@angular/compiler-cli": "14.0.4",
		"@angular/language-service": "14.0.4",
		"@commitlint/cli": "~17.0.3",
		"@commitlint/config-conventional": "~17.0.3",
		"@nrwl/cypress": "14.3.6",
		"@nrwl/eslint-plugin-nx": "14.3.6",
		"@nrwl/jest": "14.3.6",
		"@nrwl/linter": "14.3.6",
		"@nrwl/node": "14.3.6",
		"@nrwl/web": "14.3.6",
		"@nrwl/workspace": "14.3.6",
		"@types/jest": "27.4.1",
		"@types/js-base64": "~3.3.1",
		"@types/node": "18.0.0",
		"@typescript-eslint/eslint-plugin": "5.32.0",
		"@typescript-eslint/parser": "5.24.0",
		"compression-webpack-plugin": "^10.0.0",
		"cpy-cli": "^4.1.0",
		"cypress": "^9.6.0",
		"dotenv": "16.0.1",
		"eslint": "8.18.0",
		"eslint-config-prettier": "~8.5.0",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jsdoc": "^39.3.4",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"eslint-plugin-prettier": "~4.2.1",
		"eslint-plugin-react": "latest",
		"husky": "^8.0.1",
		"jest": "27.5.1",
		"jest-preset-angular": "11.1.2",
		"lint-staged": "~13.0.3",
		"markdownlint": "^0.26.0",
		"markdownlint-cli": "^0.31.1",
		"ng-packagr": "14.0.2",
		"npm-check-updates": "~16.0.5",
		"nx": "14.3.6",
		"pinst": "^3.0.0",
		"postcss": "^8.4.14",
		"postcss-import": "14.1.0",
		"postcss-preset-env": "7.7.2",
		"postcss-scss": "^4.0.4",
		"postcss-url": "10.1.3",
		"prettier": "2.7.1",
		"remark": "~14.0.2",
		"remark-cli": "~11.0.0",
		"remark-lint": "~9.1.1",
		"remark-lint-maximum-line-length": "~3.1.2",
		"remark-preset-lint-consistent": "~5.1.1",
		"remark-preset-lint-recommended": "~6.1.2",
		"rimraf": "~3.0.2",
		"rollup": "~2.79.0",
		"rollup-plugin-gzip": "~3.0.1",
		"rollup-plugin-terser": "~7.0.2",
		"standard-version": "~9.5.0",
		"stylelint": "~14.9.1",
		"stylelint-config-prettier": "~9.0.3",
		"stylelint-prettier": "~2.0.0",
		"ts-jest": "27.1.4",
		"ts-node": "10.8.1",
		"typedoc": "~0.23.10",
		"typescript": "4.7.4",
		"webpack-bundle-analyzer": "~4.5.0",
		"yaml-lint": "^1.7.0"
	}
}
