{
	"name": "aegis-auth",
	"description": "Angular authentication libaries",
	"version": "0.0.1",
	"license": "MIT",
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/auth#readme",
	"repository": "github:AlexAegis/auth",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/auth/issues"
	},
	"private": true,
	"config": {
		"engine-strict": true
	},
	"engines": {
		"node": ">=10.13.0",
		"yarn": ">=1.0.0"
	},
	"workspaces": {
		"packages": [
			"apps/*",
			"libs/*"
		]
	},
	"scripts": {
		"ng": "ng",
		"nx": "nx",
		"start": "ng serve",
		"start:core": "nx serve core-demo",
		"build": "ng build",
		"build:prod": "ng build --prod",
		"test": "ng test",
		"lint": "nx workspace-lint && ng lint",
		"lint:ts": "tslint --project .",
		"lint:es": "eslint .",
		"lint:css": "stylelint --allow-empty-input --syntax=css ./**/*.css",
		"lint:scss": "stylelint --allow-empty-input --syntax=scss ./**/*.scss",
		"lint:md": "markdownlint --ignore './node_modules' .",
		"lint:rmd": "remark .",
		"lint:format": "prettier --check .",
		"e2e": "ng e2e",
		"affected:apps": "nx affected:apps",
		"affected:libs": "nx affected:libs",
		"affected:build": "nx affected:build",
		"affected:build:prod": "nx affected:build --prod",
		"affected:e2e": "nx affected:e2e",
		"affected:test": "nx affected:test",
		"affected:lint": "nx affected:lint",
		"affected:dep-graph": "nx affected:dep-graph",
		"affected": "nx affected",
		"format": "nx format:write",
		"format:write": "nx format:write",
		"format:check": "nx format:check",
		"ncu": "ncu",
		"ncu:u": "ncu -u",
		"update": "ng update @nrwl/workspace",
		"workspace-schematic": "nx workspace-schematic",
		"dep-graph": "nx dep-graph",
		"help": "nx help",
		"postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
		"release": "standard-version"
	},
	"dependencies": {
		"@angular/animations": "^9.1.9",
		"@angular/common": "^9.1.9",
		"@angular/compiler": "^9.1.9",
		"@angular/core": "^9.1.9",
		"@angular/forms": "^9.1.9",
		"@angular/platform-browser": "^9.1.9",
		"@angular/platform-browser-dynamic": "^9.1.9",
		"@angular/router": "^9.1.9",
		"@nrwl/angular": "9.3.0",
		"core-js": "^3.6.5",
		"rxjs": "~6.5.5",
		"tslib": "^2.0.0",
		"zone.js": "^0.10.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "0.901.7",
		"@angular-devkit/build-ng-packagr": "~0.901.7",
		"@angular/cli": "9.1.7",
		"@angular/compiler-cli": "^9.1.9",
		"@angular/language-service": "^9.1.9",
		"@commitlint/cli": "^8.3.5",
		"@commitlint/config-conventional": "^8.3.4",
		"@nrwl/cypress": "9.3.0",
		"@nrwl/eslint-plugin-nx": "9.3.0",
		"@nrwl/jest": "9.3.0",
		"@nrwl/linter": "^9.3.0",
		"@nrwl/node": "^9.3.0",
		"@nrwl/web": "^9.3.0",
		"@nrwl/workspace": "9.3.0",
		"@types/jest": "25.2.3",
		"@types/node": "~14.0.5",
		"@typescript-eslint/eslint-plugin": "3.0.0",
		"@typescript-eslint/parser": "3.0.0",
		"codelyzer": "~5.2.2",
		"cypress": "^4.6.0",
		"dotenv": "8.2.0",
		"eslint": "7.1.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.3",
		"husky": "^4.2.5",
		"jest": "26.0.1",
		"jest-preset-angular": "8.2.0",
		"lint-staged": "^10.2.6",
		"markdownlint": "^0.20.3",
		"markdownlint-cli": "^0.23.1",
		"ng-packagr": "^9.1.5",
		"npm-check-updates": "^6.0.1",
		"prettier": "^2.0.5",
		"remark": "^12.0.0",
		"remark-cli": "^8.0.0",
		"remark-lint": "^7.0.0",
		"remark-lint-maximum-line-length": "^2.0.0",
		"remark-preset-lint-consistent": "^3.0.0",
		"remark-preset-lint-recommended": "^4.0.0",
		"rollup": "^2.10.9",
		"rollup-plugin-gzip": "^2.5.0",
		"rollup-plugin-terser": "^6.0.1",
		"standard-version": "^8.0.0",
		"stylelint": "^13.5.0",
		"stylelint-config-prettier": "^8.0.1",
		"stylelint-prettier": "^1.1.2",
		"ts-jest": "26.0.0",
		"ts-node": "~8.10.1",
		"tslint": "~6.1.2",
		"tslint-config-prettier": "^1.18.0",
		"tslint-plugin-prettier": "^2.3.0",
		"typescript": "~3.8.3",
		"yaml-lint": "^1.2.4"
	}
}
