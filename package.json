{
	"name": "aegis-auth",
	"description": "Angular authentication libaries",
	"version": "1.0.1",
	"license": "MIT",
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/auth#readme",
	"repository": "github:AlexAegis/auth",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/auth/issues"
	},
	"private": true,
	"config": {
		"engine-strict": true
	},
	"engines": {
		"node": ">=10.13.0",
		"yarn": ">=1.0.0"
	},
	"workspaces": {
		"packages": [
			"apps/*",
			"libs/*"
		]
	},
	"scripts": {
		"ng": "ng",
		"nx": "nx",
		"start": "nx serve jwt-demo",
		"start:core": "nx serve core-demo",
		"start:jwt": "nx serve jwt-demo",
		"start:ngrx": "nx serve ngrx-demo",
		"build": "nx affected:build --all --prod",
		"build:jwt": "nx build jwt-demo --prod --stats-json",
		"clean": "rimraf ./dist ./coverage ./.codacy-coverage ./typedocs ./cache",
		"self-destruct": "rimraf ./dist ./coverage ./.codacy-coverage ./typedocs ./cache ./node-modules ./package-lock.json ./yarn.lock ./pnpm-lock.yaml",
		"test": "nx run-many --all --target test",
		"test:skip-cache": "nx run-many --all --target test --skip-nx-cache",
		"lint": "nx workspace-lint && ng lint",
		"lint:ts": "tslint --project tsconfig.base.json",
		"lint:es": "eslint .",
		"lint:css": "stylelint --allow-empty-input --syntax=css ./**/*.css",
		"lint:scss": "stylelint --allow-empty-input --syntax=scss ./**/*.scss",
		"lint:md": "markdownlint --ignore 'CHANGELOG.md' --ignore-path '.gitignore' .",
		"lint:rmd": "remark .",
		"lint:format": "prettier --check .",
		"e2e": "nx e2e",
		"analyze:jwt": "webpack-bundle-analyzer dist/apps/jwt-demo/stats.json",
		"affected:apps": "nx affected:apps",
		"affected:libs": "nx affected:libs",
		"affected:build": "nx affected:build --prod",
		"affected:e2e": "nx affected:e2e",
		"affected:test": "nx affected:test",
		"affected:lint": "nx affected:lint",
		"affected:dep-graph": "nx affected:dep-graph",
		"affected": "nx affected",
		"format": "nx format:write",
		"format:write": "nx format:write",
		"format:check": "nx format:check",
		"ncu": "ncu -u",
		"typedoc": "typedoc libs",
		"update": "ng update @nrwl/workspace",
		"update:all": "nx update --all --force",
		"workspace-schematic": "nx workspace-schematic",
		"dep-graph": "nx dep-graph",
		"help": "nx help",
		"postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points --tsconfig tsconfig.base.json",
		"release": "standard-version",
		"prepublishOnly": "yarn build"
	},
	"dependencies": {
		"@angular/common": "~10.0.14",
		"@angular/compiler": "~10.0.14",
		"@angular/core": "~10.0.14",
		"@angular/forms": "~10.0.14",
		"@angular/platform-browser": "~10.0.14",
		"@angular/platform-browser-dynamic": "~10.0.14",
		"@angular/router": "~10.0.14",
		"@nrwl/angular": "10.1.0",
		"core-js": "~3.6.5",
		"rxjs": "~6.6.2",
		"tslib": "^2.0.1",
		"zone.js": "~0.11.1"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.1000.8",
		"@angular-devkit/build-ng-packagr": "~0.1000.8",
		"@angular/cli": "~10.1.0",
		"@angular/compiler-cli": "~10.0.14",
		"@angular/language-service": "~10.1.0",
		"@commitlint/cli": "~9.1.2",
		"@commitlint/config-conventional": "~9.1.2",
		"@nrwl/cypress": "~10.1.0",
		"@nrwl/eslint-plugin-nx": "~10.1.0",
		"@nrwl/jest": "~10.1.0",
		"@nrwl/linter": "~10.1.0",
		"@nrwl/node": "~10.1.0",
		"@nrwl/web": "~10.1.0",
		"@nrwl/workspace": "~10.1.0",
		"@semantic-release/npm": "~7.0.5",
		"@types/jest": "~26.0.13",
		"@types/js-base64": "~3.0.0",
		"@types/node": "~14.6.3",
		"@typescript-eslint/eslint-plugin": "~4.0.1",
		"@typescript-eslint/parser": "~4.0.1",
		"codelyzer": "^6.0.0",
		"cypress": "~5.1.0",
		"dotenv": "~8.2.0",
		"eslint": "~7.8.1",
		"eslint-config-prettier": "~6.11.0",
		"eslint-plugin-prettier": "~3.1.4",
		"husky": "~4.2.5",
		"jest": "~26.4.2",
		"jest-preset-angular": "8.3.1",
		"lint-staged": "~10.2.13",
		"markdownlint": "~0.20.4",
		"markdownlint-cli": "~0.23.2",
		"ng-packagr": "^10.1.0",
		"npm-check-updates": "~8.0.3",
		"prettier": "~2.1.1",
		"remark": "~12.0.1",
		"remark-cli": "~8.0.1",
		"remark-lint": "~7.0.1",
		"remark-lint-maximum-line-length": "~2.0.3",
		"remark-preset-lint-consistent": "~3.0.1",
		"remark-preset-lint-recommended": "~4.0.1",
		"rimraf": "~3.0.2",
		"rollup": "~2.26.9",
		"rollup-plugin-gzip": "~2.5.0",
		"rollup-plugin-terser": "~7.0.1",
		"standard-version": "~9.0.0",
		"stylelint": "~13.7.0",
		"stylelint-config-prettier": "~8.0.2",
		"stylelint-prettier": "~1.1.2",
		"ts-jest": "~26.3.0",
		"ts-node": "~9.0.0",
		"tslint": "~6.1.3",
		"tslint-config-prettier": "~1.18.0",
		"tslint-plugin-prettier": "~2.3.0",
		"typedoc": "~0.19.0",
		"typedoc-plugin-external-module-name": "~4.0.3",
		"typescript": "~3.9.7",
		"webpack-bundle-analyzer": "~3.8.0",
		"yaml-lint": "~1.2.4"
	}
}
