{
	"name": "aegis-auth",
	"description": "Angular authentication libraries",
	"version": "12.0.0",
	"license": "MIT",
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/auth#readme",
	"repository": "github:AlexAegis/auth",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/auth/issues"
	},
	"keywords": [
		"ng",
		"angular",
		"nx",
		"jwt",
		"token",
		"auth",
		"authentication",
		"rxjs",
		"reactive",
		"interceptor",
		"ts",
		"typescript",
		"js",
		"javascript"
	],
	"sideEffects": [
		"./apps/*/src/main.ts"
	],
	"private": true,
	"config": {
		"engine-strict": true
	},
	"engines": {
		"node": ">=14.0.0",
		"npm": ">=7.0.0"
	},
	"workspaces": [
		"apps/*",
		"libs/*"
	],
	"scripts": {
		"ng": "ng",
		"nx": "nx",
		"start": "nx serve jwt-demo",
		"start:core": "nx serve core-demo",
		"start:jwt": "nx serve jwt-demo",
		"start:ngrx": "nx serve ngrx-demo",
		"build": "nx affected:build --all --configuration production",
		"build:core": "nx build core-demo --configuration production --stats-json",
		"build:jwt": "nx build jwt-demo --configuration production --stats-json",
		"build:ngrx": "nx build ngrx-demo --configuration production --stats-json",
		"hoist:core": "cpy ./dist/apps/core-demo/* ./dist",
		"clean": "rimraf ./dist ./coverage ./.codacy-coverage ./cache",
		"nuke": "rimraf ./dist ./coverage ./.codacy-coverage ./cache ./node-modules ./package-lock.json ./yarn.lock ./pnpm-lock.yaml",
		"test": "nx run-many --all --target test",
		"test:skip-cache": "nx run-many --all --target test --skip-nx-cache",
		"lint": "nx workspace-lint && nx lint",
		"lint:css": "stylelint --allow-empty-input --syntax=css {apps,libs}/**/*.css",
		"lint:scss": "stylelint --allow-empty-input --syntax=scss {apps,libs}/**/*.scss",
		"lint:md": "markdownlint --ignore 'CHANGELOG.md' --ignore-path '.gitignore' .",
		"lint:rmd": "remark .",
		"lint:format": "prettier --check .",
		"e2e": "nx e2e",
		"analyze:jwt": "webpack-bundle-analyzer dist/apps/jwt-demo/stats.json",
		"affected:apps": "nx affected:apps",
		"affected:libs": "nx affected:libs",
		"affected:build": "nx affected:build --configuration production",
		"affected:e2e": "nx affected:e2e",
		"affected:test": "nx affected:test",
		"affected:lint": "nx affected:lint",
		"affected:dep-graph": "nx affected:dep-graph",
		"affected": "nx affected",
		"format": "nx format:write",
		"format:write": "nx format:write",
		"format:check": "nx format:check",
		"ncu": "ncu -u",
		"typedoc": "typedoc --tsconfig ./tsconfig.base.json --out ./dist/typedoc",
		"update": "ng update @nrwl/workspace",
		"update:all": "nx update --all --force",
		"dep-graph": "nx dep-graph",
		"help": "nx help",
		"postinstall": "husky install && node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
		"release": "standard-version",
		"release:minor": "standard-version --release-as minor",
		"release:major": "standard-version --release-as major",
		"prepublishOnly": "pinst --disable && npm run build",
		"postpublish": "pinst --enable",
		"workspace-generator": "nx workspace-generator"
	},
	"dependencies": {
		"@angular/animations": "12.2.16",
		"@angular/cdk": "12.2.13",
		"@angular/common": "12.2.16",
		"@angular/compiler": "12.2.16",
		"@angular/core": "12.2.16",
		"@angular/forms": "12.2.16",
		"@angular/material": "12.2.13",
		"@angular/platform-browser": "12.2.16",
		"@angular/platform-browser-dynamic": "12.2.16",
		"@angular/router": "12.2.16",
		"@fortawesome/angular-fontawesome": "^0.9.0",
		"@fortawesome/fontawesome-svg-core": "^1.2.35",
		"@fortawesome/free-brands-svg-icons": "^5.15.3",
		"@fortawesome/free-solid-svg-icons": "^5.15.3",
		"@nrwl/angular": "12.10.1",
		"core-js": "~3.22.4",
		"js-base64": "^3.7.2",
		"rxjs": "~7.5.5",
		"tslib": "^2.2.0",
		"zone.js": "~0.11.4"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "12.2.17",
		"@angular-eslint/builder": "12.7.0",
		"@angular-eslint/eslint-plugin": "12.3.1",
		"@angular-eslint/eslint-plugin-template": "12.3.1",
		"@angular-eslint/schematics": "12.7.0",
		"@angular-eslint/template-parser": "12.3.1",
		"@angular/cli": "12.2.17",
		"@angular/compiler-cli": "12.2.16",
		"@angular/language-service": "12.2.16",
		"@commitlint/cli": "~12.1.1",
		"@commitlint/config-conventional": "~12.1.2",
		"@nrwl/cypress": "12.10.1",
		"@nrwl/eslint-plugin-nx": "12.10.1",
		"@nrwl/jest": "12.10.1",
		"@nrwl/linter": "12.10.1",
		"@nrwl/node": "12.10.1",
		"@nrwl/web": "12.10.1",
		"@nrwl/workspace": "12.10.1",
		"@types/jest": "27.0.2",
		"@types/js-base64": "~3.0.0",
		"@types/node": "15.0.1",
		"@typescript-eslint/eslint-plugin": "4.31.2",
		"@typescript-eslint/parser": "4.31.2",
		"compression-webpack-plugin": "^6.1.1",
		"cpy-cli": "^4.1.0",
		"cypress": "^6.0.1",
		"dotenv": "10.0.0",
		"eslint": "~7.25.0",
		"eslint-config-prettier": "~8.3.0",
		"eslint-plugin-cypress": "^2.11.2",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsdoc": "^33.0.0",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"eslint-plugin-prettier": "~3.4.0",
		"eslint-plugin-react": "latest",
		"husky": "^6.0.0",
		"jest": "27.2.3",
		"jest-preset-angular": "10.0.1",
		"lint-staged": "~10.5.4",
		"markdownlint": "^0.25.1",
		"markdownlint-cli": "^0.31.1",
		"ng-packagr": "12.2.7",
		"npm-check-updates": "~11.5.10",
		"pinst": "^2.1.6",
		"prettier": "2.6.2",
		"remark": "~13.0.0",
		"remark-cli": "~9.0.0",
		"remark-lint": "~8.0.0",
		"remark-lint-maximum-line-length": "~2.0.3",
		"remark-preset-lint-consistent": "~4.0.0",
		"remark-preset-lint-recommended": "~5.0.0",
		"rimraf": "~3.0.2",
		"rollup": "~2.46.0",
		"rollup-plugin-gzip": "~2.5.0",
		"rollup-plugin-terser": "~7.0.2",
		"standard-version": "~9.2.0",
		"stylelint": "~13.13.1",
		"stylelint-config-prettier": "~8.0.2",
		"stylelint-prettier": "~1.2.0",
		"ts-jest": "27.0.5",
		"ts-node": "9.1.1",
		"typedoc": "~0.22.15",
		"typescript": "4.3.5",
		"webpack-bundle-analyzer": "~4.4.1",
		"yaml-lint": "^1.6.0"
	}
}
