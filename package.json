{
	"name": "aegis-auth",
	"description": "Angular authentication libraries",
	"version": "16.0.1",
	"license": "MIT",
	"author": {
		"email": "alexaegis@pm.me",
		"name": "AlexAegis",
		"url": "https://www.github.com/AlexAegis"
	},
	"homepage": "https://www.github.com/AlexAegis/auth#readme",
	"repository": "github:AlexAegis/auth",
	"bugs": {
		"url": "https://www.github.com/AlexAegis/auth/issues"
	},
	"keywords": [
		"ng",
		"angular",
		"nx",
		"jwt",
		"token",
		"auth",
		"authentication",
		"rxjs",
		"reactive",
		"interceptor",
		"ts",
		"typescript",
		"js",
		"javascript"
	],
	"sideEffects": [
		"./apps/*/src/main.ts"
	],
	"private": true,
	"config": {
		"engine-strict": true
	},
	"engines": {
		"node": ">=14.0.0",
		"npm": ">=7.0.0"
	},
	"workspaces": [
		"apps/*",
		"libs/*"
	],
	"scripts": {
		"ng": "ng",
		"nx": "nx",
		"start": "nx serve jwt-demo",
		"start:core": "nx serve core-demo",
		"start:jwt": "nx serve jwt-demo",
		"start:ngrx": "nx serve ngrx-demo",
		"build": "nx run-many --all --parallel 4 --target build --configuration production",
		"build:core": "nx build core-demo --configuration production --stats-json",
		"build:jwt": "nx build jwt-demo --configuration production --stats-json",
		"build:ngrx": "nx build ngrx-demo --configuration production --stats-json",
		"hoist:core": "cpy 'dist/apps/core-demo/*' 'dist'",
		"clean": "rimraf ./dist ./coverage ./.codacy-coverage ./cache",
		"nuke": "rimraf ./dist ./coverage ./.codacy-coverage ./cache ./node_modules ./package-lock.json ./yarn.lock ./pnpm-lock.yaml",
		"test": "nx run-many --all --parallel 4 --target test",
		"lint": "nx run-many --all --parallel 4 --target lint",
		"lint:css": "stylelint --allow-empty-input {apps,libs}/**/*.css",
		"lint:scss": "stylelint --allow-empty-input --customSyntax=postcss-scss {apps,libs}/**/*.scss",
		"lint:md": "markdownlint --ignore 'CHANGELOG.md' --ignore-path '.gitignore' .",
		"lint:rmd": "remark .",
		"lint:format": "prettier --check .",
		"e2e": "nx e2e",
		"analyze:jwt": "webpack-bundle-analyzer dist/apps/jwt-demo/stats.json",
		"affected:apps": "nx affected:apps",
		"affected:libs": "nx affected:libs",
		"affected:build": "nx affected:build --configuration production",
		"affected:e2e": "nx affected:e2e",
		"affected:test": "nx affected:test",
		"affected:lint": "nx affected:lint",
		"affected:dep-graph": "nx affected:dep-graph",
		"affected": "nx affected",
		"format": "nx format:write",
		"format:write": "nx format:write",
		"format:check": "nx format:check",
		"ncu": "ncu -u",
		"typedoc": "typedoc --tsconfig ./tsconfig.base.json --out ./dist/typedoc",
		"update": "nx migrate latest",
		"dep-graph": "nx dep-graph",
		"help": "nx help",
		"postinstall": "husky install",
		"release": "standard-version",
		"release:minor": "standard-version --release-as minor",
		"release:major": "standard-version --release-as major",
		"prepublishOnly": "pinst --disable && npm run build",
		"postpublish": "pinst --enable",
		"workspace-generator": "nx workspace-generator"
	},
	"dependencies": {
		"@angular/animations": "17.3.1",
		"@angular/cdk": "17.3.1",
		"@angular/common": "17.3.1",
		"@angular/compiler": "17.3.1",
		"@angular/core": "17.3.1",
		"@angular/forms": "17.3.1",
		"@angular/material": "17.3.1",
		"@angular/platform-browser": "17.3.1",
		"@angular/platform-browser-dynamic": "17.3.1",
		"@angular/router": "17.3.1",
		"@fortawesome/angular-fontawesome": "^0.14.1",
		"@fortawesome/fontawesome-svg-core": "^6.5.1",
		"@fortawesome/free-brands-svg-icons": "^6.5.1",
		"@fortawesome/free-solid-svg-icons": "^6.5.1",
		"@nx/angular": "17.3.2",
		"core-js": "~3.36.1",
		"js-base64": "^3.7.7",
		"rxjs": "~7.8.1",
		"tslib": "^2.6.2",
		"zone.js": "0.14.4"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "17.3.1",
		"@angular-devkit/core": "17.3.1",
		"@angular-devkit/schematics": "17.3.1",
		"@angular-eslint/builder": "^17.3.0",
		"@angular-eslint/eslint-plugin": "^17.3.0",
		"@angular-eslint/eslint-plugin-template": "^17.3.0",
		"@angular-eslint/schematics": "^17.3.0",
		"@angular-eslint/template-parser": "17.3.0",
		"@angular/cli": "~17.3.1",
		"@angular/compiler-cli": "17.3.1",
		"@angular/language-service": "17.3.1",
		"@commitlint/cli": "~19.2.1",
		"@commitlint/config-conventional": "~19.1.0",
		"@nx/workspace": "17.3.2",
		"@nx/js": "17.3.2",
		"@nx/web": "17.3.2",
		"@nx/cypress": "17.3.2",
		"@nx/linter": "17.3.2",
		"@nx/eslint-plugin": "17.3.2",
		"@nx/jest": "17.3.2",
		"@nx/node": "17.3.2",
		"@schematics/angular": "17.3.1",
		"@types/jest": "29.5.12",
		"@types/js-base64": "~3.3.1",
		"@types/node": "20.11.30",
		"@typescript-eslint/eslint-plugin": "7.3.1",
		"@typescript-eslint/parser": "7.3.1",
		"compression-webpack-plugin": "^11.1.0",
		"cpy-cli": "^5.0.0",
		"cypress": "^13.7.1",
		"dotenv": "16.4.5",
		"eslint": "8.57.0",
		"eslint-config-prettier": "~9.1.0",
		"eslint-plugin-cypress": "2.15.1",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-jsdoc": "^48.2.1",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"eslint-plugin-prettier": "~5.1.3",
		"eslint-plugin-react": "latest",
		"husky": "^9.0.11",
		"jest": "29.7.0",
		"jest-environment-jsdom": "29.7.0",
		"jest-preset-angular": "14.0.3",
		"lint-staged": "~15.2.2",
		"markdownlint": "^0.34.0",
		"markdownlint-cli": "^0.39.0",
		"ng-packagr": "17.3.0",
		"npm-check-updates": "~16.14.17",
		"nx": "18.1.2",
		"pinst": "^3.0.0",
		"postcss": "^8.4.38",
		"postcss-import": "16.1.0",
		"postcss-preset-env": "9.5.2",
		"postcss-scss": "^4.0.9",
		"postcss-url": "10.1.3",
		"prettier": "3.2.5",
		"remark": "~15.0.1",
		"remark-cli": "~12.0.0",
		"remark-lint": "~9.1.2",
		"remark-lint-maximum-line-length": "~3.1.3",
		"remark-preset-lint-consistent": "~5.1.2",
		"remark-preset-lint-recommended": "~6.1.3",
		"rimraf": "~5.0.5",
		"rollup": "~4.13.0",
		"rollup-plugin-gzip": "~3.1.2",
		"rollup-plugin-terser": "~7.0.2",
		"standard-version": "~9.5.0",
		"stylelint": "~16.2.1",
		"stylelint-config-prettier": "~9.0.5",
		"stylelint-prettier": "~5.0.0",
		"ts-jest": "29.1.2",
		"ts-node": "10.9.2",
		"typedoc": "~0.25.12",
		"typescript": "^5.4.3",
		"webpack-bundle-analyzer": "~4.10.1",
		"yaml-lint": "^1.7.0"
	}
}
